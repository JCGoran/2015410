proc insertsyn_apic(){
nsyn=$1
for i=0, nsyn-1{
	synca1[i]= new Exp2Syn (.5)
	synca1[i].tau1= 0.5
	synca1[i].tau2= 5
	synca1[i].e= 0
	ncca1[i]= new NetCon (ns, synca1[i], 0, 0, 0)
   j=0
   while(j==0){
   location=int(rc.repick())   
   tmp=rb.repick()  
   peso=$2
   access apic[oblist.x[location]] { 
   j=1
   synca1[i].loc(tmp)
   print secname(),location, tmp, distance(tmp), peso
   ncca1[i].weight=peso
   }
   }
   
}
}

proc insertsyn2_nr(){
j=0
